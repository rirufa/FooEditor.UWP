<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FooEditor.UWP">

    <local:TextPointConverter x:Key="TextPointConverter"/>
    <local:RateConverter x:Key="RateConverter"/>
    <local:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <local:LineFeedConverter x:Key="LineFeedConverter"/>

    <SolidColorBrush x:Key="ActiveTabBackgroundBrush" Color="White" />
    <SolidColorBrush x:Key="PointOverForgroundBrush" Color="Red" />
    <SolidColorBrush x:Key="PressedForgroundBrush" Color="DarkRed" />

</ResourceDictionary>